#include <stdio.h>

struct Trid
{
   int x;
   int y;
   int z;
};


int main (void)
{
   int soma =0;
   struct Trid v1 = {1,0,5};
   struct Trid v2 = {3,3,3};
   struct Trid v3 = {0,10,0};
   

   v1.z = v1.z + 10;
   v2.z = v2.z + 10;
   v3.z = v3.z + 10;
   
   printf("\n\n(%d, %d, %d)\n", v1.y, v2.y, v3.y);   
   
   printf("\n\n(%d, %d, %d)\n", v2.x, v2.y, v2.z);
   
      
   printf("\n\n(%d, %d, %d)\n", v1.z, v2.z, v3.z);
   return 0;
}
