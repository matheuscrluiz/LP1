#include <stdio.h>

int ehprimo(int numero)
{
	int qtd =0;
	int i;	

	for(i=1; i<=numero;i++)
		{
			if (numero % i == 0)
				{
					qtd = qtd + 1;
				}	
		}
	return qtd;
}

void todoprimo(int p)
{
	int qtd = 0, i;
		
	for(i=1; i<=p;i++)
		{
			if(ehprimo(i) == 2)
				{
					printf("\n %d", i);
				}
		}
}

int main (void)
{

	int max;
	printf("Digite o valor maximo: ");
	scanf("%d", &max);
	todoprimo(max);

	return 0;
}
